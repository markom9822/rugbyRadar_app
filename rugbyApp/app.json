{
  "expo": {
    "name": "Rugby Radar",
    "slug": "rugbyApp",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/rugbyIcon.png",
    "userInterfaceStyle": "dark",
    "scheme": "rugbyApp",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/rugbySplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.markom.rugbyApp",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/rugbyAdaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "userInterfaceStyle": "dark"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "a3eba83f-297a-42cb-a199-67b14518f573"
      }
    }
  }
}