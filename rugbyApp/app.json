{
  "expo": {
    "name": "rugbyApp",
    "slug": "rugbyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rugbyIcon.png",
    "userInterfaceStyle": "dark",
    "scheme": "rugbyApp",
    "splash": {
      "image": "./assets/rugbySplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.markom.rugbyApp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/rugbyAdaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
